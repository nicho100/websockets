<div class="container">
    <br>
    <% if(produc.length===0){%>
        <h2 style="color:red">No se encontraron productos</h2>
    <% } else{%>
    <table class="table table-dark">
        <thead>
          <tr>
            
            <th scope="col">Name</th>
            <th scope="col">Price</th>
            <th scope="col">Thumbnail</th>
          </tr>
        </thead>
        <tbody>
            <% produc.forEach((produc,index)=>{%>
          <tr>
            
            <td><%=produc.name%></td>
            <td><%=produc.price%></td>
            <td><img src="produc.thumbnail"  width="70" height="70"></td>
          </tr>
          <%})%>
        </tbody>
        <%} %>
      </table>
</div>
